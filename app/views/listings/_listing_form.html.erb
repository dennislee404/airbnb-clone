<%= form_with model: @listing do |f| %>
	<%= f.label :title %>
	<%= f.text_field :title %>
	<br><br>
	<%= f.label :description %>
	<%= f.text_area :description %>
	<br><br>
	<%= f.label :room %>
	<%= f.number_field :room %>
	<br><br>
	<%= f.label :capacity %>
	<%= f.number_field :capacity %>
	<br><br>
	<%= f.label :availability %>
	<%= f.check_box :availability %>
	<br><br>
	<%= f.label :property_type %>
	<%= f.text_field :property_type %>
	<br><br>
	<%= f.label :price %>
	<%= f.number_field :price %>
	<br><br>
	<%= f.label :location %>
	<%= f.text_field :location %>
	<br><br>
	<% if @listing.new_record? %>
		<%= f.submit 'Create Listing' %>
	<% else %>
		<%=	f.submit 'Edit Listing' %>
	<% end %>
<% end %>